  .top_menu--bar{
    box-shadow:  0 22px 44px -20px rgba(0,0,0,0.29);
    ul{
      list-style: none;
      padding: 10px 20px;
      display: flex;
      white-space: nowrap;
      overflow: auto;
      margin: 0;
      li{
        margin: 0;
        margin-right: 35px;
        a{
          @include font-gothic-bold();
          font-size: $small_size;
          text-transform: uppercase;
          color: black;
          span{
            display: none;
          }
        }
      }
    }
  }
  .dropdown_container{
    display: block;
    position: fixed;
    top: 0;
    z-index: 100000;
    height: 100%;
    background: white;
    left: -400px;
    max-width: 400px;
    transition: all 0.2s ease-out;
    &.active{
      left: 0;
    }
    .dropdown--header{
      padding: 20px;
      display: flex;
      align-items: center;
      justify-content: space-between;
      h2{
        margin: 0;
        font-size: $body_size;
      }
      .close{
        position: relative;
        display: flex;
        align-items: center;
        text-decoration: underline;
        &::before{
          content: " ";
          width: 18px;
          height: 1px;
          background: black;
          transform: rotate(45deg);
          display: block;
          margin-right: 5px;
        }
        &::after{
          content: " ";
          width: 18px;
          height: 1px;
          transform: rotate(-45deg);
          background: black;
          display: block;
          position: absolute;
          top: 47%;
          left: 0;
        }
      }
    }
    .dropdown{
      position: initial;
      background: transparent;
      .header_search_form{
        margin: auto;
        border: 2px solid black;
        border-radius: 3px;
        padding: 0;
        width: calc(100% - 30px);
        .search-terms{
          height: 45px;
          min-height: 45px;
          font-size: $body_size;
          font-weight: normal;
          color: black;
          margin: 0;
        }
      }
      .menu{
        margin: 5px 0;
        overflow: auto;
        height: 100vh;
        li{
          a{
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 10px 20px;
            &.active{
              span{
                &::after{
                  content: none;
                }
              } 
            }
            span{
              display: flex;
              align-items: center;
              justify-content: center;
              width: 10px;
              height: 10px;
              padding: 0!important;
              position: relativel;
              &::before{
                content: " ";
                width: 10px;
                height: 2px;
                background: black;
                display: block;
                position: absolute;
                letter-spacing: 0;
                margin: 0;
                left: 0;
              }
              &::after{
                content: " ";
                width: 2px;
                height: 10px;
                background: black;
                display: block;
              }
            }
          }
          .mobile-mega-menu{
            background: #F5F7FB;
          }
        }
      }
    }
  }
}
