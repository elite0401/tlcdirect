            font-size: $h3_size;
            letter-spacing: 0.77px;
          }
        }
        .toggle{
          @include font-gothic-bold();
          font-size: $regular_size;
          letter-spacing: 0.6px;
          color: #030303;
        }
        .toggle_list{
          padding: 0;
          li{
            a{
              label{
                margin-left: 5px; 
                display: flex;
                font-size: $body_size;
                @include font-gothic-regular();
                letter-spacing: 0.68px;
                color: #030303;
                position: relative;
                &::before{
                  content: " ";
                  width: 20px;
                  height: 20px;
                  display:block;
                  background: white;
                  border: 1px solid #122F54;
                  margin-right: 10px;
                }
              }
              &.active{
                label{
                  &::after{
                    content: " ";
                    width: 10px;
                    height: 5px;
                    border: 1px solid #122f54;
                    display: block;
                    position: absolute;
                    left: 5px;
                    border-top: none;
                    top: 6px;
                    border-right: none;
                    transform: rotate(-45deg);
                  }
                }
              }
            }
          }
          .clear-active-filter{
            display: flex;
            font-weight: normal;
            font-size: $nav_size;
            align-items: center;
            text-transform: uppercase;
            line-height: 16px;
            transform: none;
            top: -35px;
            .x-icon{
                display: flex;
                align-items: center;
                justify-content: center;
                transform: rotate(45deg);
                top: 0;
                &::before{
                  content: "";
                  width: 15px;
                  height: 1px;
                  background: black;
                  display: block;
                  position: initial;
                }
                &::after{
                  content: "";
                  width: 1px;
                  height: 15px;
                  background: black;
                  display: block;
                  position: absolute;
                }
            }
          }
        }
      }
    }
