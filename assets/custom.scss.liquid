                  padding: 20px 4px;
                  padding-left: 20px;
                  margin-left: -10px;
                  
                  width: calc(50% + 10px);
                  @include respond-to("medium-down") {
                    width: 100%;
                    margin: 0;
                    padding: 10px;
                  }
                } 
                &:nth-last-child(1){
                  background: #F5F7FB;
                  padding: 20px 4px;
                  margin-right: -10px;
                  padding-right: 20px;
                  width: calc(50% + 10px);
                  @include respond-to("medium-down") {
                    width: 100%;
