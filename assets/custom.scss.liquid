$site-width: 1600px;
$gutter: 20px;

$grid-small: 768px;
$grid-medium: 1023px;
$grid-large: 1300px;  
$grid-widescreen: 1400px;

$small: 'small';
$small-down: 'small-down';
$small-up: 'small-up';
$medium: 'medium';
$medium-down: 'medium-down';
$medium-up: 'medium-up';
$large: 'large';
$large-down: 'large-down';
$large-up: 'large-up';
$widescreen-up: 'widescreen-up';
$widescreen-down: 'widescreen-down';

$breakpoints: (
  $small-down '(max-width: #{$grid-small})',
  $small '(min-width: #{$grid-small + 1}) and (max-width: #{$grid-medium})',
  $small-up '(min-width: #{$grid-small + 1})',
  $medium-down '(max-width: #{$grid-medium})',
  $medium '(min-width: #{$grid-medium + 1}) and (max-width: #{$grid-large})',
  $medium-up '(min-width: #{$grid-medium + 1})',
  $large-down '(max-width: #{$grid-large})',
  $large '(min-width: #{$grid-large + 1}) and (max-width: #{$grid-widescreen})',
  $large-up '(min-width: #{$grid-large + 1})',
  $widescreen-down '(max-width: #{$grid-widescreen})',
  $widescreen-up '(min-width: #{$grid-widescreen + 1})'
);

@mixin respond-to($media-query) {
  $breakpoint-found: false;

  @each $breakpoint in $breakpoints {
    $name: nth($breakpoint, 1);
    $declaration: nth($breakpoint, 2);

    @if $media-query == $name and $declaration {
      $breakpoint-found: true;

      @media only screen and #{$declaration} {
        @content;
      }
    }
  }

  @if $breakpoint-found == false {
    @warn 'Breakpoint "#{$media-query}" does not exist';
  }
}

$color-white: #F9F9F9;
$color-blue: #122F54;
$color-light_blue: #E9EFFA;
$color-pink: #CC1291;
$color-light_pink: #F7E6F3;
$color-black: #030303;
 



@mixin font-gothic-bold {
  font-family: var(--headline__font);
  font-weight: bold;
}

@mixin font-gothic-regular {
  font-family: var(--headline__font);
  font-weight: normal;
}

@mixin font-berkeley-regular {
  font-family: var(--regular__font);
  font-weight: 400;
}



$body_size: calc(var(--regular_font_size) * 1px);
$h1_size: calc(var(--h1_size) * 1px);
$h2_size: calc(var(--h1_size) * 1px);
$h3_size: calc(var(--h3_size) * 1px);
$h4_size: calc(var(--h4_size) * 1px);
$h5_size: calc(var(--h5_size) * 1px);
$h6_size: calc(var(--h6_size) * 1px);
$hero_size: calc(var(--page_headline_size) * 1px);
$link_size: calc(var(--regular_font_size) * 0.875px);
$regular_size: calc(var(--regular_font_size) * 0.875px);
$small_size: calc(var(--regular_font_size) * 0.75px);
$nav_size: calc(var(--nav__font_size) * 1px);
body{
  @include font-gothic-regular();
  font-size: $body_size;
  line-height: 26px;
  letter-spacing: normal;
}

.hero{
  font-size: $hero_size;
  line-height: 43px;
  letter-spacing: 1.5px;
}

strong, .bold{
  @include font-gothic-bold();
}

a{
  @include font-gothic-bold();
  font-weight: 400;
  font-size: $link_size;
  line-height: 22px;
  color: var(--link_color);
  &.underline{
    text-decoration: underline;
  }
}

p, li {
  @include font-berkeley-regular();
  font-size: calc(var(--regular_font_size) * 1px);
}

a.button, input[type="submit"].button, .button, .action_button{
  background-color: var(--btn_color);
  border-color: var(--btn_color);
  color: $color-white;
  height: 48px;
  letter-spacing: 1.78px;
  cursor: pointer;
  span{
    color: inherit;
  }
  &:hover{
    background-color: var(--btn_hover_color);
    border-color: var(--btn_hover_color);
  }
  @include font-gothic-bold();
  max-width: 240px;
  width: 100%;
  @include respond-to("medium-down"){
    max-width: initial;
  }
  &.center{
    margin: 10px auto;
    left: 50%;
    transform: translateX(-50%);
  }
  &.border{
    border: 3px solid;
    color: var(--regular-color);
    background: transparent;
    padding: 0 10px;
  }
  &.full{
    max-width: 100%;
    width: 100%;
    margin-bottom: 20px;
  }
  
  &.blue{
    background: var(--banner_button_color);
    border-color: var(--banner_button_color);
    
    &:hover{
      background-color: var(--banner_btn_hover_color);
      border-color: var(--banner_btn_hover_color);
    }
    &.light{
      background: transparent;
      position: relative;
      color: var(--banner_button_color);
      border: none;
      &::after{
        content: "";
        position: absolute;
        background-color: var(--banner_button_color);
        width: 100%;
        height: 100%;
        opacity: 0.1;
        z-index: -1;
      }
    }
  }
  &.pink{
    background-color: var(--btn_color);
    border-color: var(--btn_color);
    &:hover{
      background-color: var(--btn_hover_color);
      border-color: var(--btn_hover_color);
    }
    &.light{
      background: transparent;
      position: relative;
      color: var(--btn_color);
      &::after{
        content: "";
        position: absolute;
        background-color: var(--btn_color);
        width: 100%;
        height: 100%;
        opacity: 0.1;
        z-index: -1;
      }
      &:hover{
        color: white;
        &::after{
          opacity: 1;
        }
      }
    }
  }
}


