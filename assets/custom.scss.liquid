  display: none!important;
}

.not-found-template{
  margin-bottom: 100px;
  @media screen and (max-width: 768px) {
    margin-bottom: 0;
  }
}

.product-list.flickity-enabled{
  display: block;
  .thumbnail{
    max-width: 300px;
    margin: 0 10px;
  }
}

.newsletter_section {
  .newsletter-text p {
    @include font-gothic-regular();
  }
}

.paginate {
  display: flex;
  align-items: center;
  justify-content: end;
  @include respond-to("small-down") {
    justify-content: center;
  }
  .deco {
    margin-left: 10px;
  }
  .prev , .next{
    padding: 0;
  }
  .next{
    margin-left: 10px;
  }
  .page {
    width: 48px;
    height: 48px;
    display: block;
    margin: 0;
    position: relative;
    margin-left: 10px;
    border: 1px solid $color-blue!important;
    color: $color-blue;
    border-radius: 50%;
    padding: 0;
    @include respond-to("small-down") {
      width: 35px;
      height: 35px;
    }
    &:hover{
      background: rgba(49,88,167,0.08)!important;
    }
    &.current{
      background: rgba(49,88,167,0.08)!important;
      display: flex;
      align-items: center;
      justify-content: center;
      font-weight: normal;
      color: $color-blue!important;
    }
    a{
      border: none;
      margin: 0;
      position: absolute;
      width: 100%;
      height: 100%;
      padding: 0;
      left: 0;
      top: 0;
      display: flex;
      align-items: center;
      justify-content: center;
      background: transparent!important;
      border: none!important;
      color: $color-blue!important;
    }
  }
}

#shopify-section-page-about-template {
  position: relative;
  &::before{
    content: " ";
    width: 100%;
    display: block;
    box-shadow: inset 0 20px 50px -30px rgba(0, 0, 0, 0.04);
    height: 125px;
    position: absolute;
    top: 0;
  }
  div.content.page {
    padding: 30px;
  }
}

.shopify-section--collection-template{
  margin-bottom: 100px;
  .thumbnail{
    &:hover{
      .product-info__caption {
        .title{
          color: #A83A86; 
        }
      }
    }
  }
}

